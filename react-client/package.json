{
  "name": "ums-gui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9001",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && eslint . && vite build",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "dev:player": "vite --config vite.config.player.ts",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:runserver:macos": "export RUNNING_TESTS=true; npm run private:removeconfig:macos;cd ..;./runtestserver.sh",
    "test:prepareserver:macos": "rm -rf ../target/ums-*-jar*;cd ../;mvn -P macos package -DskipTests -Djaronly;cd react-client;npm run private:copy-bin:macos;npm run private:copy-jar;npm run private:ensure-file-permissions",
    "test:prepareserver:macos-arm": "rm -rf ../target/ums-*-jar*;cd ../;mvn -P macos-arm package -DskipTests;cd react-client;npm run private:copy-bin:macos-arm;npm run private:copy-jar;npm run private:ensure-file-permissions",
    "private:copy-bin:macos": "mkdir -p ../osx;cp -f ../target/bin/osx/10.15/ffmpeg ../osx/ffmpeg;cp -f ../target/bin/osx/youtube-dl ../osx/youtube-dl",
    "private:copy-bin:macos-arm": "mkdir -p ../osx;cp -f ../target/bin/osx/arm/ffmpeg ../osx/ffmpeg;cp -f ../target/bin/osx/youtube-dl ../osx/youtube-dl",
    "private:copy-jar": "cp -f ../target/ums-*-jar-with-dependencies.jar ../ums-testserver.jar",
    "private:ensure-file-permissions": "chmod +x ../runtestserver.sh;chmod +x ../ums-testserver.jar",
    "private:removeconfig:macos": "rm -rf ~/Library/'Application Support'/UMS-tests"
  },
  "packageManager": "yarn@4.10.2",
  "dependencies": {
    "@mantine/core": "8.3.1",
    "@mantine/dropzone": "8.3.1",
    "@mantine/form": "8.3.1",
    "@mantine/hooks": "8.3.1",
    "@mantine/modals": "8.3.1",
    "@mantine/notifications": "8.3.1",
    "@microsoft/fetch-event-source": "2.0.1",
    "@tabler/icons-react": "^3.34.1",
    "axios": "1.12.2",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "react": "19.1.1",
    "react-country-flag": "3.1.0",
    "react-dom": "^19.1.1",
    "react-movable": "3.4.1",
    "react-router-dom": "7.9.1",
    "react-use-websocket": "4.13.0",
    "video.js": "8.23.4"
  },
  "devDependencies": {
    "@playwright/test": "1.55.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "@types/lodash": "4.17.20",
    "@types/react": "19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "9.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "typescript": "5.8.3",
    "typescript-eslint": "8.46.1",
    "vite": "7.1.6"
  }
}
