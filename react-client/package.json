{
  "name": "ums-gui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9001",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && eslint . && vite build",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "test:runserver:macos": "export RUNNING_TESTS=true; npm run private:removeconfig:macos;cd ..;./runtestserver.sh",
    "test:prepareserver:macos": "cd ../;mvn -P macos package -DskipTests -Doffline -Djaronly;cd react-client;npm run private:copy-jar;npm run private:ensure-file-permissions",
    "test:prepareserver:macos-arm": "cd ../;mvn -P macos-arm package -DskipTests -Doffline;cd react-client;npm run private:copy-jar;npm run private:ensure-file-permissions",
    "private:copy-jar": "cp -f ../target/ums-*-jar-with-dependencies.jar ../ums-testserver.jar",
    "private:ensure-file-permissions": "chmod +x ../runtestserver.sh;chmod +x ../ums-testserver.jar",
    "private:removeconfig:macos": "rm -rf ~/Library/'Application Support'/UMS-tests"
  },
  "packageManager": "yarn@4.4.0",
  "dependencies": {
    "@mantine/core": "7.12.2",
    "@mantine/dropzone": "7.12.2",
    "@mantine/form": "7.12.2",
    "@mantine/hooks": "7.12.2",
    "@mantine/modals": "7.12.2",
    "@mantine/notifications": "7.12.2",
    "@microsoft/fetch-event-source": "2.0.1",
    "axios": "1.7.7",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "react": "18.3.1",
    "react-country-flag": "3.1.0",
    "react-dom": "18.3.1",
    "react-movable": "3.3.1",
    "react-router-dom": "6.26.1",
    "tabler-icons-react": "1.56.0",
    "video.js": "8.17.3"
  },
  "devDependencies": {
    "@playwright/test": "1.47.0",
    "@types/lodash": "4.17.7",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "9.9.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.9",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "5.4.3"
  }
}
