{
  "name": "ums-gui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9001",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && eslint . && vite build",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "dev:player": "vite --config vite.config.player.ts",
    "test:runserver:macos": "export RUNNING_TESTS=true; npm run private:removeconfig:macos;cd ..;./runtestserver.sh",
    "test:prepareserver:macos": "cd ../;mvn -P macos package -DskipTests -Doffline -Djaronly;cd react-client;npm run private:copy-jar;npm run private:ensure-file-permissions",
    "test:prepareserver:macos-arm": "cd ../;mvn -P macos-arm package -DskipTests -Doffline;cd react-client;npm run private:copy-jar;npm run private:ensure-file-permissions",
    "private:copy-jar": "cp -f ../target/ums-*-jar-with-dependencies.jar ../ums-testserver.jar",
    "private:ensure-file-permissions": "chmod +x ../runtestserver.sh;chmod +x ../ums-testserver.jar",
    "private:removeconfig:macos": "rm -rf ~/Library/'Application Support'/UMS-tests"
  },
  "packageManager": "yarn@4.6.0",
  "dependencies": {
    "@mantine/core": "7.17.2",
    "@mantine/dropzone": "7.17.2",
    "@mantine/form": "7.17.2",
    "@mantine/hooks": "7.17.2",
    "@mantine/modals": "7.17.2",
    "@mantine/notifications": "7.17.2",
    "@microsoft/fetch-event-source": "2.0.1",
    "@tabler/icons-react": "^3.31.0",
    "axios": "1.8.3",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "react": "19.0.0",
    "react-country-flag": "3.1.0",
    "react-dom": "19.0.0",
    "react-movable": "3.4.1",
    "react-router-dom": "7.3.0",
    "video.js": "8.22.0"
  },
  "devDependencies": {
    "@playwright/test": "1.51.0",
    "@stylistic/eslint-plugin": "4.1.0",
    "@types/lodash": "4.17.16",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.22.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0",
    "vite": "6.2.2"
  }
}
